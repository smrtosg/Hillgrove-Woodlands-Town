		(L.L.engine_luefter_clamped) !
		{if}
			0 (S.L.engine_luefter)
		{endif}
	{endif}

	(L.L.engine_luefter)
	{if}
		(L.L.brake) 0.05 >
		(L.L.velocity) 15 > &&
		{if}
			(L.L.engine_n) 1.4 * 1300 min (S.L.radiator_RPM_sollwert)
		{else}
			(L.L.engine_n) 1300 min (S.L.radiator_RPM_sollwert)
		{endif}
	{endif}

	(L.L.radiator_RPM) (L.L.radiator_RPM_sollwert) >
