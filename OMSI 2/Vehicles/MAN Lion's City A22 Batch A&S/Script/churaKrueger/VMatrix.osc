			2 l1 + $cutEnd (S.$.Matrix_TerminusL2)
		{endif}
' *U := Add U-Bahn icon
		(L.$.Matrix_TerminusL2) $d 2 $SetLengthR "*U" $=
		{if}
			2 $cutEnd (S.$.Matrix_TerminusL2)
			"U" (L.$.Matrix_Icon) $+ (S.$.Matrix_Icon)
		{endif}
' *T := Add train icon
		(L.$.Matrix_TerminusL2) $d 2 $SetLengthR "*T" $=
		{if}
