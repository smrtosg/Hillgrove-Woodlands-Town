			{else}
				(L.L.Matrix_L3Bold) ! (S.L.Matrix_L3Bold)
				2 $cutEnd (S.$.Matrix_TerminusL3)
			{endif}
		{endif}

		(L.$.Matrix_TerminusL3) 1 $SetLengthR "]" $=
		{if}
' *C := line text colour
		(L.$.Matrix_TerminusL3) "*C[" (M.L.Matrix_IsNextCmd)
		{if}
				(L.L.Matrix_LED) (L.L.Matrix_LEDMonochrome) ! &&
				{if}
